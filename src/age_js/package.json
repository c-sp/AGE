{
    "name": "age-js",
    "version": "0.0.0",
    "license": "Apache 2.0",
    "scripts": {
        "ng": "ng",
        "http-serve-dist": "http-server ./dist --gzip",
        "build": "npm run generate-files && ng build --prod",
        "build:analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/stats.json",
        "start:dev": "npm run generate-files && ng serve",
        "start:prod": "npm run build && npm run gzip-directory && npm run http-serve-dist",
        "test": "ng test",
        "lint": "ng lint",
        "generate-files": "npm run write-icons && npm run write-version",
        "write-icons": "node tools/write-icons.js",
        "write-version": "node tools/write-version.js",
        "cmake-age-wasm": "node tools/cmake-age-wasm.js",
        "gzip-directory": "node tools/gzip-directory.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "6.0.7",
        "@angular/common": "6.0.7",
        "@angular/compiler": "6.0.7",
        "@angular/core": "6.0.7",
        "@angular/forms": "6.0.7",
        "@angular/http": "6.0.7",
        "@angular/platform-browser": "6.0.7",
        "@angular/platform-browser-dynamic": "6.0.7",
        "@angular/pwa": "0.6.8",
        "@angular/router": "6.0.7",
        "@angular/service-worker": "6.0.7",
        "@fortawesome/angular-fontawesome": "0.1.1",
        "@fortawesome/fontawesome-svg-core": "1.2.0",
        "@fortawesome/free-solid-svg-icons": "5.1.0",
        "core-js": "2.5.7",
        "file-type": "8.0.0",
        "jszip": "3.1.5",
        "rxjs": "6.2.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.6.8",
        "@angular/cli": "6.0.8",
        "@angular/compiler-cli": "6.0.7",
        "@angular/language-service": "6.0.7",
        "@types/file-type": "5.2.1",
        "@types/jasmine": "2.8.8",
        "@types/jasminewd2": "2.0.3",
        "@types/jszip": "3.1.4",
        "@types/node": "10.5.1",
        "codelyzer": "4.4.2",
        "http-server": "0.11.1",
        "jasmine-core": "3.1.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "2.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.1",
        "karma-firefox-launcher": "1.1.0",
        "karma-jasmine": "1.1.2",
        "karma-jasmine-html-reporter": "1.1.0",
        "rxjs-tslint": "0.1.5",
        "svg2png": "4.1.1",
        "ts-node": "7.0.0",
        "tslint": "5.10.0",
        "typescript": "2.7.2",
        "webpack-bundle-analyzer": "2.13.1"
    }
}
