{
    "name": "age-js",
    "version": "0.0.0",
    "license": "Apache 2.0",
    "scripts": {
        "ng": "ng",
        "http-serve-dist": "http-server ./dist/age-js --gzip",
        "build": "npm run generate-files && ng build --prod",
        "build:analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/age-js/stats.json",
        "start:dev": "npm run generate-files && ng serve",
        "start:prod": "npm run build && npm run gzip-directory && npm run http-serve-dist",
        "test": "ng test",
        "lint": "ng lint",
        "generate-files": "npm run write-icons && npm run write-version",
        "write-icons": "node tools/write-icons.js",
        "write-version": "node tools/write-version.js",
        "cmake-age-wasm": "node tools/cmake-age-wasm.js",
        "gzip-directory": "node tools/gzip-directory.js"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "7.2.7",
        "@angular/common": "7.2.7",
        "@angular/compiler": "7.2.7",
        "@angular/core": "7.2.7",
        "@angular/forms": "7.2.7",
        "@angular/http": "7.2.7",
        "@angular/platform-browser": "7.2.7",
        "@angular/platform-browser-dynamic": "7.2.7",
        "@angular/pwa": "0.13.4",
        "@angular/router": "7.2.7",
        "@angular/service-worker": "7.2.7",
        "@fortawesome/angular-fontawesome": "0.3.0",
        "@fortawesome/fontawesome-svg-core": "1.2.15",
        "@fortawesome/free-solid-svg-icons": "5.7.2",
        "core-js": "2.6.5",
        "file-type": "10.9.0",
        "jszip": "3.2.0",
        "rxjs": "6.4.0",
        "tslib": "1.9.3",
        "zone.js": "0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.13.4",
        "@angular/cli": "7.3.4",
        "@angular/compiler-cli": "7.2.7",
        "@angular/language-service": "7.2.7",
        "@types/file-type": "10.6.0",
        "@types/jasmine": "3.3.9",
        "@types/jszip": "3.1.5",
        "@types/node": "11.10.4",
        "codelyzer": "4.5.0",
        "http-server": "0.11.1",
        "jasmine-core": "3.3.0",
        "jasmine-spec-reporter": "4.2.1",
        "karma": "4.0.1",
        "karma-chrome-launcher": "2.2.0",
        "karma-coverage-istanbul-reporter": "2.0.5",
        "karma-firefox-launcher": "1.1.0",
        "karma-jasmine": "2.0.1",
        "karma-jasmine-html-reporter": "1.4.0",
        "rxjs-tslint": "0.1.7",
        "sharp": "0.21.3",
        "ts-node": "8.0.2",
        "tslint": "5.13.1",
        "typescript": "3.2.4",
        "webpack-bundle-analyzer": "3.1.0"
    }
}
