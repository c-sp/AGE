{
  "name": "age-js",
  "version": "0.0.0",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "http-serve-dist": "cd ./dist && serve",
    "build": "npm run generate-files && ng build --prod",
    "build:analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/age-js/stats-es2015.json",
    "start:dev": "npm run generate-files && ng serve",
    "start:prod": "npm run build && npm run gzip-directory && npm run http-serve-dist",
    "test": "ng test",
    "lint": "ng lint",
    "generate-files": "npm run write-icons && npm run write-version",
    "write-icons": "node tools/write-icons.js",
    "write-version": "node tools/write-version.js",
    "cmake-age-wasm": "node tools/cmake-age-wasm.js",
    "gzip-directory": "node tools/gzip-directory.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.9",
    "@angular/cdk": "9.2.4",
    "@angular/common": "9.1.9",
    "@angular/compiler": "9.1.9",
    "@angular/core": "9.1.9",
    "@angular/forms": "9.1.9",
    "@angular/material": "9.2.4",
    "@angular/platform-browser": "9.1.9",
    "@angular/platform-browser-dynamic": "9.1.9",
    "@angular/router": "9.1.9",
    "@angular/service-worker": "9.1.9",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "jszip": "3.4.0",
    "resize-observer-polyfill": "1.5.1",
    "rxjs": "6.5.5",
    "tslib": "1.13.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.7",
    "@angular/cli": "9.1.7",
    "@angular/compiler-cli": "9.1.9",
    "@types/jasmine": "3.5.10",
    "@types/jszip": "3.4.1",
    "@types/node": "12.12.45",
    "codelyzer": "5.2.2",
    "serve": "11.3.2",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.2",
    "karma": "5.0.9",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.3.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "rxjs-tslint": "0.1.8",
    "sharp": "0.25.3",
    "ts-node": "8.10.2",
    "tslint": "6.1.2",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "3.8.0"
  }
}
