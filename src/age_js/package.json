{
  "name": "age-js",
  "version": "0.0.0",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "http-serve-dist": "cd ./dist && serve",
    "build": "npm run generate-files && ng build --prod",
    "build:analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/age-js/stats.json",
    "start:dev": "npm run generate-files && ng serve",
    "start:prod": "npm run build && npm run gzip-directory && npm run http-serve-dist",
    "test": "ng test",
    "lint": "ng lint",
    "generate-files": "npm run write-icons && npm run write-version",
    "write-icons": "node tools/write-icons.js",
    "write-version": "node tools/write-version.js",
    "cmake-age-wasm": "node tools/cmake-age-wasm.js",
    "gzip-directory": "node tools/gzip-directory.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.0.1",
    "@angular/cdk": "8.0.1",
    "@angular/common": "8.0.1",
    "@angular/compiler": "8.0.1",
    "@angular/core": "8.0.1",
    "@angular/forms": "8.0.1",
    "@angular/platform-browser": "8.0.1",
    "@angular/platform-browser-dynamic": "8.0.1",
    "@angular/pwa": "0.800.3",
    "@angular/router": "8.0.1",
    "@angular/service-worker": "8.0.1",
    "@fortawesome/angular-fontawesome": "0.4.0",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "file-type": "12.0.0",
    "jszip": "3.2.1",
    "resize-observer-polyfill": "1.5.1",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.3",
    "@angular/cli": "8.0.3",
    "@angular/compiler-cli": "8.0.1",
    "@angular/language-service": "8.0.1",
    "@types/jasmine": "3.3.13",
    "@types/jszip": "3.1.6",
    "@types/node": "12.0.8",
    "codelyzer": "5.1.0",
    "serve": "11.0.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "rxjs-tslint": "0.1.7",
    "sharp": "0.22.1",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "3.3.2"
  }
}
